{
  "name": "wtbx-type-safe-fetch",
  "version": "1.3.0",
  "type": "module",
  "description": "The simplest fetch wrapper. 最簡單的 fetch 包裝器 (更: 新增 pathParams 中間件與類型至 TsFetchTemplate)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "readme.md"],
  "exports": {
    ".": "./dist/index.js",
    "./middlewares/log": {
      "types": "./dist/middlewares/log.d.ts",
      "import": "./dist/middlewares/log.js"
    },
    "./middlewares/method-url": {
      "types": "./dist/middlewares/method-url.d.ts",
      "import": "./dist/middlewares/method-url.js"
    },
    "./middlewares/mock": {
      "types": "./dist/middlewares/mock.d.ts",
      "import": "./dist/middlewares/mock.js"
    },
    "./middlewares/params-and-body-parser": {
      "types": "./dist/middlewares/params-and-body-parser.d.ts",
      "import": "./dist/middlewares/params-and-body-parser.js"
    },
    "./middlewares/path-params-url": {
      "types": "./dist/middlewares/path-params-url.d.ts",
      "import": "./dist/middlewares/path-params-url.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-package": "tsup src/index.ts src/middlewares --format esm --dts --clean",
    "publish-package": "pnpm run build-package && npm publish"
  },
  "keywords": [
    "wtbx",
    "typescript",
    "fetch"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.19.23",
    "query-string": "^9.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
