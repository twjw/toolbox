{
  "name": "wtbx-type-safe-fetch",
  "version": "1.0.1",
  "type": "module",
  "description": "最簡單的 fetch 包裝器。(更：改 build 為 js 上架)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./middlewares/method-url": {
      "types": "./dist/middlewares/method-url.d.ts",
      "import": "./dist/middlewares/method-url.js"
    },
    "./middlewares/params-and-body-parser": {
      "types": "./dist/middlewares/params-and-body-parser.d.ts",
      "import": "./dist/middlewares/params-and-body-parser.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-package": "tsup src/index.ts src/middlewares --format esm --dts --clean",
    "publish-package": "pnpm run build-package && npm publish"
  },
  "keywords": [
    "wtbx",
    "typescript",
    "fetch"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.19.23",
    "query-string": "^9.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  },
  "peerDependencies": {
    "query-string": ">=8"
  }
}
